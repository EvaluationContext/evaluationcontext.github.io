	partition dates = calculated
		mode: import
		source =
				
				var startdt = DATE( YEAR( TODAY() ) -1, 1, 1 )
				var enddt = DATE( YEAR( TODAY() ) + 2, 1, 1 ) -1
				return
				ADDCOLUMNS(
				    CALENDAR( startdt, enddt )
				    ,"Year", YEAR( [Date] )
				    ,"Year Month", EOMONTH( [Date], 0 )
				)

